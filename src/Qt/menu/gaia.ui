   <string notr="true">QFrame {
	background-color: #25292B;
}

#centralWidget, QStackedWidget, QStackedWidget &gt; QWidget {
	background: transparent;
}

QLabel {
	color: #F8F8F8;
	background: transparent;
}

QFrame[titleFrame=true] {
	background-color: #5F5F5F;
}

QWidget[fontSize=&quot;14&quot;] {
	font-size: 14pt;
}

QWidget[fontSize=&quot;12&quot;] {
	font-size: 12pt;
}

QGroupBox {
	color: #F8F8F8;
	border: none;
	margin: 15px;
}

QWidget[inList=true] {
	font-size: 12pt;
	min-height: 60px;
	max-height: 100px;
}


/* PushButton */

QPushButton {
	background-color: #25292B;
	border: 2px solid #F8F8F8;
	color: #F8F8F8;
}

QPushButton:hover {
	background-color: #F8F8F8;
	color: #25292B;
}

QPushButton:pressed {
	background-color: #66ff66;
	color: #25292B;
	border: 2px solid #00F0F0;
}


/* RadioButton */

QRadioButton {
	background-color: #25292B;
	border: 2px solid #F8F8F8;
	color: #F8F8F8;
}

QRadioButton:hover {
	background-color: #F8F8F8;
	color: #25292B;
}

QRadioButton:checked {
	background-color: #66ff66;
	color: #25292B;
	border: 2px solid #00F0F0;
}

QRadioButton::indicator {
	width: 0px;
	height: 0px;
	color: #F8F8F8;
}


/* CheckBox */

QCheckBox {
	background-color: #25292B;
	border: 2px solid #F8F8F8;
	color: #F8F8F8;
}

QCheckBox:hover {
	background-color: #F8F8F8;
	color: #25292B;
}

QCheckBox:checked {
	background-color: #66ff66;
	color: #25292B;
	border: 2px solid #00F0F0;
}

QCheckBox::indicator {
	width: 0px;
	height: 0px;
}

/* Slider */
QSlider::groove:horizontal {
    border: 1px solid #999999;
    height: 35px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #B1B1B1, stop:1 #c4c4c4);
    margin: 2px 0;
}

QSlider::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #66ff66);
    border: 1px solid #5c5c5c;
    width: 35px;
    margin: -2px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
    border-radius: 0px;
}

/* Page Selectors */

QRadioButton[pageSelector=true] {
	background-color: #25292B;
	max-width: 90px;
	max-height: 90px;
	border-radius: 40px;
	border: none;
	padding: 10px;
}

QRadioButton[pageSelector=true]:checked {
	background-color: #F8F8F8;
	max-width: 90px;
	max-height: 90px;
	border-radius: 40px;
	border: none;
	padding: 10px;
}

#viewRadioButton {
	image: url(:Qt/menu/icons/view-white.svg);
}

#viewRadioButton:checked {
	image: url(:Qt/menu/icons/view-black.svg);
}

#toolsRadioButton {
